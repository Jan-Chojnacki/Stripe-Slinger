# The full URL of your Grafana Cloud instance (e.g., https://my-org.grafana.net)
grafana_url = "https://<your-stack-id>.grafana.net"

# API Token from Service Accounts with 'Admin' or 'Editor' role
grafana_token = "glsa_xxxxxxxxxxxxxxxxxxxx"

# The primary email address where critical storage alerts will be sent
alert_email_address = "your-email@example.com"

# The name of your Prometheus data source (found in Connections -> Data Sources)
prometheus_name = "grafanacloud-yourstack-prom"
